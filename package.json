{
  "name": "secure-frontend",
  "version": "1.0.0",
  "description": "企业级安全前端资源加密存储解决方案，专为需要高度安全性的Web应用程序设计",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "node scripts/dev.js",
    "build": "node scripts/build/build.js",
    "build:prod": "NODE_ENV=production node scripts/build/build.js --production",
    "start": "node scripts/start.js",
    "deploy": "node scripts/deploy/deploy.js",
    "deploy:staging": "node scripts/deploy/deploy.js staging",
    "deploy:prod": "node scripts/deploy/deploy.js production --auto-rollback",
    "deploy:docker": "node scripts/deploy/deploy.js production --docker",
    "rollback": "node scripts/deploy/deploy.js production --rollback",
    "encrypt": "node src/core/encryption.js",
    "upload": "node src/core/upload.js",
    "serve": "node server/index.js",
    "test": "node scripts/testing/test-runner.js",
    "test:unit": "node scripts/testing/test-runner.js --type unit",
    "test:integration": "node scripts/testing/test-runner.js --type integration",
    "test:e2e": "node scripts/testing/test-runner.js --type e2e",
    "test:security": "node scripts/runners/run-security-test.js",
    "test:performance": "node scripts/runners/run-performance-test.js",
    "lint": "node scripts/runners/run-code-quality.js",
    "security": "node scripts/runners/run-security-audit.js",
    "debug": "node scripts/debug/debug-tools.js",
    "debug:inspect": "node scripts/debug/debug-tools.js inspect",
    "debug:profile": "node scripts/debug/debug-tools.js profile",
    "debug:monitor": "node scripts/debug/debug-tools.js monitor",
    "monitoring:setup": "node scripts/monitoring/setup-monitoring.js",
    "monitoring:setup:staging": "node scripts/monitoring/setup-monitoring.js staging",
    "monitoring:setup:production": "node scripts/monitoring/setup-monitoring.js production",
    "health:check": "node scripts/monitoring/health-check.js",
    "health:check:verbose": "node scripts/monitoring/health-check.js --verbose",
    "health:check:alert": "node scripts/monitoring/health-check.js --alert"
  },
  "keywords": [
    "encryption",
    "frontend",
    "security",
    "cloud-storage",
    "aes",
    "webcrypto"
  ],
  "author": "SecureFrontEnd Team",
  "license": "AGPL-3.0",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.913.0",
    "@azure/storage-blob": "^12.17.0",
    "ali-oss": "^6.20.0",
    "archiver": "^6.0.1",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "elliptic": "^6.6.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "joi": "^17.11.0",
    "jose": "^6.1.0",
    "pqc-dilithium": "^0.2.0",
    "pqc-kyber": "^0.7.0",
    "rate-limiter-flexible": "^4.0.1",
    "secp256k1": "^5.0.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "uuid": "^13.0.0",
    "winston": "^3.11.0",
    "yauzl": "^3.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^22.10.5",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-vue": "^6.0.1",
    "eslint": "^9.38.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "^5.7.3",
    "vite": "^7.1.11",
    "vue": "^3.5.22"
  },
  "engines": {
    "node": ">=22.12.0",
    "npm": ">=10.0.0"
  }
}
